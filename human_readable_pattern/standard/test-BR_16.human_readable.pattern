# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/16.asc

category=Baureihen[category_separator]BR 16

pattern=(([[:space:]]§|[[:space:]]P|[[:space:]]T|[[:space:]]E|[[:space:]]V|[[:space:]]VT|)[[:space:]]16(\.0)[[:space:]])|([[:space:]](BR|Baureihe)[[:space:]]16[[:space:]]([^0-9]|$))
pattern=([[:space:]]16er[[:space:]])

# BR 16.0
pattern=([[:space:]]P|[[:space:]]T|[[:space:]]E|[[:space:]]V|[[:space:]]VT|)[[:space:]]16[[:space:]]0(0[1-9]|1[0-9]|2[0-9]|3[0-9]|4[0-9]|5[0-6]|6[1-9]|7[0-1])([[:space:]]|-[0-9])

# EDV Prüfziffer entfernen: sed -e 's/\-[0-9]//g'
pattern_delete=(^([[:space:]]*§.*)|([[:space:]]*P.*)|([[:space:]]*T.*)|([[:space:]]*E.*)|([[:space:]]*V.*)|([[:space:]]*VT.*))|(-[0-9])|([[:space:]][^0-9]$)
