# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/VT75.asc

category=Baureihen[category_separator]VT75

pattern_delete=([[:space:]][^0-9]$)

pattern=([[:space:]]VT[[:space:]]{0,1}75[[:space:]]([^0-9]|$))

# Pattern #1

pattern=([[:space:]]VT[[:space:]]{0,1}75(\.([09]))[[:space:]]([^0-9]|$))

# Pattern #2

pattern=([[:space:]]VT[[:space:]]{0,1}75[[:space:]]0(0[0])([[:space:]]|-[0-9]))

pattern=([[:space:]]VT[[:space:]]{0,1}75[[:space:]]9(0[0-9]|1[012345])([[:space:]]|-[0-9]))
