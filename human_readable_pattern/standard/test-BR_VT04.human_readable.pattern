pattern=([[:space:]]VT[[:space:]]{0,1}04[[:space:]]5(0[1])([[:space:]]|-[0-9]))

pattern=([[:space:]]VT[[:space:]]{0,1}04[[:space:]]00(0[ab])([[:space:]]|-[0-9]))

pattern=([[:space:]]VT[[:space:]]{0,1}04[[:space:]]10(1[ab]|2[ab]|3[ab]|4[ab]|5[ab]|6[ab]|7[ab])([[:space:]]|-[0-9]))

