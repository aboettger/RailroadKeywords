pattern_delete=(^[[:space:]]*(§|Mo|Di|Mi|Do|Fr|S(a|o)|D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT).*)|(-[0-9])
pattern_delete=([[:space:]]*.*er[[:space:]].*J.*)|([[:space:]].*-.*)

pattern=(([[:space:]]602er[[:space:]])(|-[[:space:]])(|Jahre(|n)[[:space:]]))

# Pattern #1

pattern_not_merge=((([[:space:]](§|Mo|Di|Mi|Do|Fr|S(a|o)|D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]602(\.(0))[[:space:]])|([[:space:]](BR|Baureihe)[[:space:]]602[[:space:]])

# Pattern #2

pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]602[[:space:]]0(0[1-4]|1[2])([[:space:]]|-[0-9])
