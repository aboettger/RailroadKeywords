# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/10.asc

category=Baureihen[category_separator]10

pattern=(([[:space:]]§|[[:space:]]P|[[:space:]]T|[[:space:]]E|[[:space:]]V|[[:space:]]VT|)[[:space:]]10(\.0)[[:space:]])|([[:space:]](BR|Baureihe)[[:space:]]10[[:space:]]([^0-9]|$))
pattern=([[:space:]]10er[[:space:]])

# BR 10.0
pattern=([[:space:]]P|[[:space:]]T|[[:space:]]E|[[:space:]]V|[[:space:]]VT|)[[:space:]]10[[:space:]]0(0[1-2])([[:space:]]|-[0-9])

# EDV Prüfziffer entfernen: sed -e 's/\-[0-9]//g'
pattern_delete=(^([[:space:]]*§.*)|([[:space:]]*P.*)|([[:space:]]*T.*)|([[:space:]]*E.*)|([[:space:]]*V.*)|([[:space:]]*VT.*))|(-[0-9])|([[:space:]][^0-9]$)
