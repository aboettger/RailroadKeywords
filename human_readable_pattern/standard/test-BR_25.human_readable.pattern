# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/25.asc

category=Baureihen[category_separator]25

pattern=(([[:space:]]§|[[:space:]]P|[[:space:]]T|[[:space:]]E|[[:space:]]V|[[:space:]]VT|)[[:space:]]25(\.(0|10))[[:space:]])|([[:space:]](BR|Baureihe)[[:space:]]25[[:space:]]([^0-9]|$))
pattern=([[:space:]]25er[[:space:]])

# BR 25.0
pattern=([[:space:]]P|[[:space:]]T|[[:space:]]E|[[:space:]]V|[[:space:]]VT|)[[:space:]]25[[:space:]]0(01)([[:space:]]|-[0-9])
# BR 25.10
pattern=([[:space:]]P|[[:space:]]T|[[:space:]]E|[[:space:]]V|[[:space:]]VT|)[[:space:]]25[[:space:]]10(0[1-2])([[:space:]]|-[0-9])

# EDV Prüfziffer entfernen: sed -e 's/\-[0-9]//g'
pattern_delete=(^([[:space:]]*§.*)|([[:space:]]*P.*)|([[:space:]]*T.*)|([[:space:]]*E.*)|([[:space:]]*V.*)|([[:space:]]*VT.*))|(-[0-9])|([[:space:]][^0-9]$)
