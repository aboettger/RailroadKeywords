# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/VT70.asc

category=Baureihen[category_separator]VT70

pattern_delete=([[:space:]][^0-9]$)

pattern=([[:space:]]VT[[:space:]]{0,1}70[[:space:]]([^0-9]|$))

# Pattern #1

pattern=([[:space:]]VT[[:space:]]{0,1}70(\.([059]))[[:space:]]([^0-9]|$))

# Pattern #2

pattern=([[:space:]]VT[[:space:]]{0,1}70[[:space:]]0(0[0-4])([[:space:]]|-[0-9]))
pattern=([[:space:]]VT[[:space:]]{0,1}70[[:space:]]5(0[1])([[:space:]]|-[0-9]))

pattern=([[:space:]]VT[[:space:]]{0,1}70[[:space:]]9(0[0-3]|1[0-9]|2[0-9]|3[0-9]|4[0-9]|5[01]|7[01]|8[01]|9[0])([[:space:]]|-[0-9]))
