category=Baureihen[category_separator]V90

pattern_delete=([[:space:]][^0-9]$)

pattern=([[:space:]]V[[:space:]]{0,1}90[[:space:]]([^0-9]|$))

pattern=([[:space:]]V[[:space:]]{0,1}90(\.(0|9))[[:space:]]([^0-9]|$))

pattern=([[:space:]]V[[:space:]]{0,1}90[[:space:]]0(0[123456789]|1[0-9]|2[0-9]|3[0-9]|4[0-9]|5[0-9]|6[0-9]|7[01])([[:space:]]|-[0-9]))
pattern=([[:space:]]V[[:space:]]{0,1}90[[:space:]]9(0[123])([[:space:]]|-[0-9]))
