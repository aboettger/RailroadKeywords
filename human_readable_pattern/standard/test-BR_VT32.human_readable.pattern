# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/VT32.asc

category=Baureihen[category_separator]VT32

pattern_delete=([[:space:]][^0-9]$)

pattern=([[:space:]]VT[[:space:]]{0,1}32[[:space:]]([^0-9]|$))

# Pattern #1

pattern=([[:space:]]VT[[:space:]]{0,1}32(\.([05]))[[:space:]]([^0-9]|$))

# Pattern #2

pattern=([[:space:]]VT[[:space:]]{0,1}32[[:space:]]0(0[0-9]|1[0-9]|2[0])([[:space:]]|-[0-9]))

pattern=([[:space:]]VT[[:space:]]{0,1}32[[:space:]]5(0[01])([[:space:]]|-[0-9]))

