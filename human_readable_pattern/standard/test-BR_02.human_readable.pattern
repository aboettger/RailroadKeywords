# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/02.asc

category=Baureihen[category_separator]02

pattern_not_merge=((([[:space:]](§|Mo|Di|Mi|Do|Fr|S(a|o)|D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]02(\.[0-1]|\.02|\.03|\.10)[[:space:]])|([[:space:]](BR|Baureihe)[[:space:]]02[[:space:]])
pattern=([[:space:]]02er[[:space:]])
# 02.0
pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]02[[:space:]]0(0[1-9]|10)([[:space:]]|-[0-9])
# 02.1
pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]02[[:space:]]1(0[1,2])([[:space:]]|-[0-9])
# 02.02
pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]02[[:space:]]02(01)([[:space:]]|-[0-9])
# 02.03
pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]02[[:space:]]03(14)([[:space:]]|-[0-9])
# 02.10
pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VB|VS|VT))|)[[:space:]]02[[:space:]]10(01)([[:space:]]|-[0-9])

