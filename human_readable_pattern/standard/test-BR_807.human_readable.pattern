pattern=(([[:space:]]807er[[:space:]])(|-[[:space:]])(|Jahre(|n)[[:space:]]))

# Pattern #1

pattern_not_merge=((([[:space:]](§|Mo|Di|Mi|Do|Fr|S(a|o)|D|E|EB|ESA|ET|ETA|H|T|V|VM|VB|VS|VT))|)[[:space:]]807(\.(0))[[:space:]])|([[:space:]](BR|Baureihe)[[:space:]]807[[:space:]])

# Pattern #2

pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VM|VB|VS|VT))|)[[:space:]]807[[:space:]]0(0[1-9]|1[0-9]|2[0-9]|3[0-9]|4[0-4])([[:space:]]|-[0-9])

