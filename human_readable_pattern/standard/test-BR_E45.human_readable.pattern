# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/E45.asc

category=Baureihen[category_separator]E45

pattern_delete=([[:space:]][^0-9]$)

pattern=([[:space:]]E[[:space:]]{0,1}45[[:space:]]([^0-9]|$))

pattern=([[:space:]]E[[:space:]]{0,1}45(\.(0|1|2))[[:space:]]([^0-9]|$))

pattern=([[:space:]]E[[:space:]]{0,1}45[[:space:]](0[123456789]|1[01234])([[:space:]]|-[0-9]))
pattern=([[:space:]]E[[:space:]]{0,1}45[[:space:]]0(0[123456789]|1[01234])([[:space:]]|-[0-9]))
pattern=([[:space:]]E[[:space:]]{0,1}45[[:space:]]1(0[123456789]|1[012345])([[:space:]]|-[0-9]))
pattern=([[:space:]]E[[:space:]]{0,1}45[[:space:]]2(0[123456789]|1[0-9]|2[0-9]|3[0-9]|4[01])([[:space:]]|-[0-9]))
