pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VM|VB|VS|VT))|)[[:space:]]650[[:space:]]1(0[0-9]|1[0-9])([[:space:]]|-[0-9])
pattern=(([[:space:]](D|E|EB|ESA|ET|ETA|H|T|V|VM|VB|VS|VT))|)[[:space:]]650[[:space:]]9(9[7])([[:space:]]|-[0-9])

