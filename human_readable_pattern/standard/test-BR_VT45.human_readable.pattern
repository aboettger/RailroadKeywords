# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/VT45.asc

category=Baureihen[category_separator]VT45

pattern_delete=([[:space:]][^0-9]$)

pattern=([[:space:]]VT[[:space:]]{0,1}45[[:space:]]([^0-9]|$))

# Pattern #1

pattern=([[:space:]]VT[[:space:]]{0,1}45(\.([5]|50[0-9]|51[01]))[[:space:]]([^0-9]|$))

# Pattern #2

pattern=([[:space:]]VT[[:space:]]{0,1}45[[:space:]]5(0[0-9]|1[01])([[:space:]]|-[0-9]))

pattern=([[:space:]]VT[[:space:]]{0,1}45[[:space:]]50(0[ab]|1[ab]|2[ab]|3[ab]|4[ab]|5[ab]|6[ab]|7[ab]|8[ab]|9[ab])([[:space:]]|-[0-9]))

pattern=([[:space:]]VT[[:space:]]{0,1}45[[:space:]]51(0[ab]|1[ab])([[:space:]]|-[0-9]))

