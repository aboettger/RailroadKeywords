# Quelle: http://www.beitraege.lokomotive.de/datenbank/baureihen/VT90.asc

category=Baureihen[category_separator]VT90

pattern_delete=([[:space:]][^0-9]$)

pattern=([[:space:]]VT[[:space:]]{0,1}90[[:space:]]([^0-9]|$))

# Pattern #1

pattern=([[:space:]]VT[[:space:]]{0,1}90(\.([5]))[[:space:]]([^0-9]|$))

# Pattern #2

pattern=([[:space:]]VT[[:space:]]{0,1}90[[:space:]]5(0[01])([[:space:]]|-[0-9]))
